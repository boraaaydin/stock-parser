@model StockParser.Domain.Dto.ProfileDto
          <div class="card">
                <div class="card-header border-0">
                    <h3 class="card-title">Ürünlerim</h3>
                    <div class="card-tools">
                        <a href="~/profile/insertowning" class="btn btn-tool btn-sm">
                            <i class="fas fa-plus"></i>
                        </a>
                        <a href="#" class="btn btn-tool btn-sm">
                            <i class="fas fa-bars"></i>
                        </a>
                    </div>
                </div>
                <div class="card-body table-responsive p-0">
                    <table class="table table-striped table-valign-middle">
                        <thead>
                            <tr>
                                <th>Satınalma Tarihi</th>
                                <th>Ürün</th>
                                <th>Satınalma Değeri</th>
                                <th>Günlük Değeri</th>
                                <th>Miktar</th>
                                <th>Kar/Zarar</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var stock in Model.Ownings)
                            {
                            <tr>
                                <td>
                                    @stock.PurchaseDate.Date.ToShortDateString()
                                </td>
                                <td>
                                    <img src="~/dist/img/default-150x150.png" alt="Product 1" class="img-circle img-size-32 mr-2">
                                    @stock.Name
                                </td>
                                <td>
                                    @stock.PurchaseValue
                                </td>
                                <td>
                                    @stock.CurrentValue
                                </td>
                                <td>
                                    @stock.PurchaseQuantity
                                </td>

                                <td>
                                    @stock.Profit
                                </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>